<script>
    (function ($) {
        var $window = $(window),
            $html = $('#wrapper');

        function resize() {
            if ($window.width() < 768) {
                return $html.removeClass('toggled');
            }

            $html.addClass('toggled');
        }

        $window
            .resize(resize)
            .trigger('resize');
    })(jQuery);
</script>

